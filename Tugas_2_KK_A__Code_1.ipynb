from google.colab import files
uploaded = files.upload()

import pandas as pd 
data = pd.read_csv("MeterB.csv",delimiter="\t",header=None) 
data = data.drop(data.columns[51], axis=1)
data

from sklearn.preprocessing import normalize

data_scaled = normalize(data)
data_scaled = pd.DataFrame(data_scaled, columns=data.columns)
data_scaled.head()

"""K-Means"""

# data.to_numpy().shape
x = data_scaled.copy()

import numpy as np
from sklearn.cluster import KMeans

kmeans = KMeans(n_clusters=3,init="random")
kmeans.fit(x)
# print(kmeans.labels_)

clusters_kmeans = x.copy()
clusters_kmeans['cluster_pred'] = kmeans.fit_predict(x)
preds_kmeans = kmeans.fit_predict(x)
print(clusters_kmeans)

import matplotlib.pyplot as plt

plt.title("K-Means")
plt.scatter(clusters_kmeans[0],clusters_kmeans[1],c=clusters_kmeans['cluster_pred'],cmap='rainbow')
plt.show()

"""K-Means Plus"""

# data.to_numpy().shape
w = data_scaled.copy()

import numpy as np
from sklearn.cluster import KMeans

kmeans_plus = KMeans(n_clusters = 3, init = 'k-means++', max_iter=100).fit(w)

clusters_kmeans_plus = w.copy()
clusters_kmeans_plus['cluster_pred'] = kmeans_plus.fit_predict(w)
preds_kmeans_plus = kmeans_plus.fit_predict(w)
print(clusters_kmeans_plus)

plt.title("K-Means_Plus")
plt.scatter(clusters_kmeans_plus[0],clusters_kmeans_plus[1],c=clusters_kmeans_plus['cluster_pred'],cmap='rainbow')
plt.show()

"""**Penggunaan Rumus Hierarchical Clustering**

Visualisasi dari rumus-rumus pada Hierarchical Clustering
 dapat berbentuk **Dendogram**. Dendogram merupakan diagram berbentuk seperti pohon dan dapat mencatat urutan penggabungan atau pemisahan setiap cluster

- Hierarchical Clustering tipe Ward
"""

import scipy.cluster.hierarchy as shc

plt.figure(figsize=(10, 7))  
plt.title("Dendrograms Ward")  
dend = shc.dendrogram(shc.linkage(data_scaled, method='ward'))

plt.figure(figsize=(10, 7))  
plt.title("Dendrograms Ward")  
dend = shc.dendrogram(shc.linkage(data_scaled, method='ward'))
plt.axhline(y=2.1, color='r', linestyle='--')

from sklearn.cluster import AgglomerativeClustering

y = data_scaled.copy()

ward = AgglomerativeClustering(n_clusters=3, affinity='euclidean', linkage='ward')
ward.fit(y)

clusters_ward = y.copy()   
clusters_ward['cluster_pred'] = ward.fit_predict(y)
print(clusters_ward)

plt.title("Ward")
plt.scatter(clusters_ward[1], clusters_ward[2], c=clusters_ward['cluster_pred'], cmap='rainbow')
data_scaled.shape

clusters_ward.shape

